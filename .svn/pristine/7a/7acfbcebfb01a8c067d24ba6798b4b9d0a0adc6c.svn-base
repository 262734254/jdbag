package com.xiu.jd.bean.ware;

import java.io.Serializable;

public class XiuAddress implements Serializable{

	private static final long serialVersionUID = 3419427048169890389L;
    //邮编
	private String postcode;
	//省份
	private String province;
	//市区
	private String city;
	//县或区
	private String area;
	public XiuAddress() {
		super();
	}
	
	public XiuAddress(String province, String city, String area) {
		super();
		this.province = province;
		this.city = city;
		this.area = area;
	}

	
	public XiuAddress(String province,  String area) {
		super();
		this.province = province;
		this.area = area;
	}
	

	public String getPostcode() {
		return postcode;
	}
	public void setPostcode(String postcode) {
		this.postcode = postcode;
	}
	public String getProvince() {
		return province;
	}
	public void setProvince(String province) {
		this.province = province;
	}
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	public String getArea() {
		return area;
	}
	public void setArea(String area) {
		this.area = area;
	}

	@Override
	public String toString() {
		return "XiuAddress [province=" + province + ", city=" + city
				+ ", area=" + area + "]";
	}
	
	
}
